			windowDef OGShowGroup {
				rect		0, 0, 0, 0
				visible		0
				notime		1
				noevents	1
				float group 0

				onTime 0 {
					set "OGGeneralOptions1::visible" "0" ;
					set "OGGeneralOptions2::visible" "0" ;
					set "OGGeneralOptions3::visible" "0" ;
					set "OGRunCrouchZoom::visible" "0" ;
					set "OGWeaponOptions::visible" "0" ;
					set "OGWeaponSmokeSelf::visible" "0" ;
					set "OGWeaponSmokeOthers::visible" "0" ;
					set "OGProjectileSmokeSelf::visible" "0" ;
					set "OGProjectileSmokeOthers::visible" "0" ;
					set "OGDamageFeedback::visible" "0" ;
					set "OGServerOptions::visible" "0" ;

					if ("OGGroupSelectionBox::visible") {
						set "OGGroupSelectionBox::visible" "0" ;
						if ("group" <= 0 || "group" > 10) {
							set "OGGeneralOptions1::visible" "1" ;
							set "OGGroupText::text" CST_STR_00200074 ;
						} else if ("group" == 1) {
							set "OGGeneralOptions2::visible" "1" ;
							set "OGGroupText::text" CST_STR_00200075 ;
						} else if ("group" == 2) {
							set "OGGeneralOptions3::visible" "1" ;
							set "OGGroupText::text" CST_STR_00200203 ;
						} else if ("group" == 3) {
							set "OGRunCrouchZoom::visible" "1" ;
							set "OGGroupText::text" CST_STR_00200076 ;
						} else if ("group" == 4) {
							set "OGWeaponOptions::visible" "1" ;
							set "OGGroupText::text" CST_STR_00200077 ;
						} else if ("group" == 5) {
							set "OGWeaponSmokeSelf::visible" "1" ;
							set "OGGroupText::text" CST_STR_00200078 ;
						} else if ("group" == 6) {
							set "OGWeaponSmokeOthers::visible" "1" ;
							set "OGGroupText::text" CST_STR_00200079 ;
						} else if ("group" == 7) {
							set "OGProjectileSmokeSelf::visible" "1" ;
							set "OGGroupText::text" CST_STR_00200080 ;
						} else if ("group" == 8) {
							set "OGProjectileSmokeOthers::visible" "1" ;
							set "OGGroupText::text" CST_STR_00200081 ;
						} else if ("group" == 9) {
							set "OGDamageFeedback::visible" "1" ;
							set "OGGroupText::text" CST_STR_00200082 ;
						} else if ("group" == 10) {
							set "OGServerOptions::visible" "1" ;
							set "OGGroupText::text" CST_STR_00200083 ;
						}
					} else {
						set "OGGroupSelectionBox::visible" "1" ;
					}
				}
			}
