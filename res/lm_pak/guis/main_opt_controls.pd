				set "mouseExitAnim" "0" ;
				set	"cmd" "play guisounds_menuclickup" ;
				transition "forecolor" "1 1 1 1" "0.6 1 1 1" "200" ;
				transition "backcolor" "0.57 0.7 0.83 0.44" "0.6 0.8 1 0.2" "400" ;
				set "CstHelpText::text" CST_STR_00200053 ;
